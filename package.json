{
    "name": "@oat-sa/tao-i18n-tools",
    "version": "0.5.0",
    "description": "Internationalisation tools for tao",
    "bin": {
        "i18n:extract": "./bin/extractTranslations.js"
    },
    "scripts": {
        "extract": "node ./bin/extractTranslations.js",
        "lint": "eslint -c .eslintrc.js --ext js .",
        "lint:report": "eslint -c .eslintrc.js --ext js --output-file eslint_report.json --format json .",
        "test": "jest",
        "test:cov": "npm test -- --coverage",
        "test:cov:ci": "npm test -- --coverage --runInBand",
        "test:update": "jest -u"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/oat-sa/tao-i18n-tools.git"
    },
    "keywords": [
        "tao",
        "i18n"
    ],
    "author": {
        "name": "Anshul Sharma"
    },
    "license": "GPL-2.0",
    "bugs": {
        "url": "https://github.com/oat-sa/tao-i18n-tools/issues"
    },
    "homepage": "https://github.com/oat-sa/tao-i18n-tools#readme",
    "engines": {
        "node": ">=10.0.0"
    },
    "dependencies": {
        "@rollup/pluginutils": "^5.0.0",
        "commander": "^4.1.1",
        "glob": "^7.2.3",
        "minimatch": "^9.0.0",
        "pofile": "^1.1.4",
        "svelte": "3.32.3"
    },
    "devDependencies": {
        "@oat-sa/eslint-config-tao": "^2.0.0",
        "@oat-sa/prettier-config": "^0.1.1",
        "eslint": "^8.39.0",
        "jest": "^29.5.0",
        "prettier": "^2.8.8"
    },
    "prettier": "@oat-sa/prettier-config"
}
